{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "firsttry",
      "camelcase": "FirstTry",
      "title": "My first try at a TS grammar",
      "scope": "source.firsttry",
      "file-types": [
        "firsttry"
      ],
      "injection-regex": "^firsttry$",
      "class-name": "TreeSitterFirsttry"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "My first try at a TS grammar",
    "authors": [
      {
        "name": "R.G. Esteves",
        "email": "rgesteves@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-firsttry"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}